{
  "name": "vitepress-plugin-nested-sidebar",
  "private": false,
  "version": "0.0.1-rc.1",
  "type": "module",
  "license": "MIT",
  "author": "LogicSpark <info@logicspark.com>",
  "description": "Plugin nested sidebar for Vitepress",
  "homepage": "https://logicspark.com",
  "url": "https://github.com/logicspark/vitepress-plugin-nested-sidebar",
  "repository": {
    "type": "git",
    "url": "https://github.com/logicspark/vitepress-plugin-nested-sidebar"
  },
  "bugs": {
    "url": "https://github.com/logicspark/vitepress-plugin-nested-sidebar"
  },
  "keywords": [
    "vitepress-plugin-sidebar",
    "vitepress-plugin",
    "vitepress"
  ],
  "exports": {
    ".": {
      "type": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/types",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "gray-matter": "^4.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "unbuild": "^1.2.1",
    "vite": "^4.4.5",
    "vitepress": "^1.0.0-rc.4"
  }
}
