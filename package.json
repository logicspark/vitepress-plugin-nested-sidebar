    "node": "^14.13.1 || ^16.7.0 || >=18"
  },
  "keywords": [
    "vitepress",
    "auto",
    "sidebar"
  ],
  "devDependencies": {
    "@types/node": "^18.16.16",
    "gray-matter": "^4.0.3",
    "vite": "^4.3.9",
    "vitepress": "1.0.0-rc.10",
    "vue-tsc": "^1.6.5"
  }
